#include <iostream>
#include <string>

using namespace std;

char getFrequent(string str){

  char mostfrequentchar = '';

  int maxcount = 0;
  

  ordered_map<char, int> a;

  for (int i = 0; i < str.length(); i++){
    a[str[i]]++;
    if (maxcount < a[str[i]]) { 
            maxcount = a[str[i]]; 
            mostfrequentchar = str[i]; 
        } 
    } 
  
    return mostfrequentchar;
    
  }

}

int main(){

  string str = "aaabbcccdde";
  cout << getFrequent(str);

  
}
