#include <vector>
using namespace std;

void LeftRotation(vector<int> v, int d){

  for (int i = 0; i <v.size(); i++){
    std::rotate(v.begin(), v.begin()+d, v.end());
    cout << v[i];
  }


}


int main(){

  int d = 4;
  vector<int> v;
  v.push_back(1);
  v.push_back(2);
  v.push_back(3);
  v.push_back(4);
  v.push_back(5);

      
  LeftRotation(v, d);

}
